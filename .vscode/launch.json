{
    "version": "0.2.0",
    "configurations": [
      
      {
        "name": "Run Chrome",
        "request": "launch",
        "type": "chrome",
        "url": "http://localhost:3000",
        "webRoot": "${workspaceFolder}"
      },
    
      {
        "name": "Django",
        "type": "python",
        "request": "launch",
        "program": "${workspaceFolder}/manage.py",
        "args": [
          "runserver",
          "127.0.0.1:8000"
        ],
        "django": true,
        "justMyCode": true
      }
    ]
  }